{
  "manifest_version": 2,
  "name": "OnTack",
  "description": "Uses Machine Learning Techniques to keep you from getting distracted",
  "version": "0.0.1",
  "permissions": [
    "clipboardWrite",
    "contextMenus",
    "notifications",
    "file://*",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
}
